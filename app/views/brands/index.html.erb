<h1>Brands</h1>
<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>Active</th>
      <!-- Add more table headers for other attributes -->
    </tr>
  </thead>
  <tbody>
    <% @brands.each do |brand| %>
      <tr>
        <td><%= brand.name %></td>
        <td><%= brand.description %></td>
        <td><%= brand.active ? 'Yes' : 'No' %></td>
        <!-- Display other attributes as needed -->
      </tr>
    <% end %>
  </tbody>
</table>

